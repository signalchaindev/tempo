{
  "name": "tempo",
  "version": "0.1.0",
  "description": "A Node Graphql framework that uses Golang to create a resolver map and concatenate the SDL from GraphQL files into a useable schema for JavaScript APIs.",
  "license": "MIT",
  "author": "",
  "homepage": "https://github.com/signalchainio/tempo#readme",
  "bugs": {
    "url": "https://github.com/signalchainio/tempo/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/signalchainio/tempo.git"
  },
  "keywords": [
    "graphql",
    "graphql server framework"
  ],
  "scripts": {
    "dev": "run-p --race api watch",
    "api": "nodemon -r esm -e js,graphql src/server.js --ignore __tempo__",
    "watch": "nodemon -r esm __tempo__/watcher.js --ignore __tempo__",
    "dev:package": "pushd .\\__tempo__\\package && go run main.go && popd && run-p --race api watch:package",
    "watch:package": "cross-env PACKAGE_DEV=true nodemon -r esm __tempo__/watcher.js --ignore __tempo__"
  },
  "dependencies": {
    "apollo-server-express": "2.14.2",
    "cors": "2.8.5",
    "cross-env": "7.0.2",
    "dotenv": "8.2.0",
    "esm": "3.2.25",
    "express": "4.17.1",
    "graphql": "14.6.0",
    "graphql-tag": "^2.10.3",
    "lodash": "4.17.15",
    "mongoose": "5.9.9",
    "node-fetch": "2.6.0",
    "nodemon": "2.0.3"
  },
  "devDependencies": {
    "@graphql-tools/merge": "^6.2.1",
    "eslint": "6.8.0",
    "eslint-config-standard": "14.1.1",
    "eslint-plugin-graphql": "3.1.1",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-json": "2.1.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.0.1",
    "npm-run-all": "4.1.5",
    "prettier": "2.0.5"
  }
}